<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE sqlMap  PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"  "http://ibatis.apache.org/dtd/sql-map-2.dtd"><sqlMap namespace="EmExpertAbilityHistory">	<typeAlias alias="elementCondition" type="com.ufgov.zc.common.system.dto.ElementConditionDto" />	<typeAlias alias="EmExpertAbilityHistory" type="com.ufgov.zc.common.zc.model.EmExpertAbilityHistory" />		<typeAlias alias="EmExpertAbility" type="com.ufgov.zc.common.zc.model.EmExpertAbility" />		<resultMap id="expertAbilityMap" class="EmExpertAbility">		  <result property="emExpertCode" column="EM_EXPERT_CODE" />           <result property="emExpertName" column="EM_EXPERT_NAME" />                 <result property="emExpertSex" column="EM_EXPERT_SEX" />           <result property="emBirthday" column="EM_BIRTHDAY" />               <result property="emIdNo" column="EM_ID_NO" />                          <result property="emUnitName" column="EM_UNIT_NAME" />             <result property="emUnitName" column="EM_UNIT_NAME" />         	</resultMap>		<resultMap id="expertAbilityHistoryMap" class="EmExpertAbilityHistory">          <result property="emExpertCode" column="EM_EXPERT_CODE"/>	             	     <result property="emExpertAbilityDesc" column="EM_EXPERT_ABILITY_DESC"/>	          <result property="emExpertRatingStartDate" column="EM_EXPERT_RATING_START_DATE"/>	          <result property="emExpertRatingEndDate" column="EM_EXPERT_RATING_END_DATE"/>	          <result property="emExpertSuperiority" column="EM_EXPERT_SUPERIORITY"/>	          <result property="emExpertImprovement" column="EM_EXPERT_IMPROVEMENT"/>	          <result property="zcInputCode" column="ZC_INPUT_CODE"/>	          <result property="zcInputName" column="ZC_INPUT_NAME"/>	          <result property="zcInputDate" column="ZC_INPUT_DATE"/>	          <result property="emExpertAbilityGrade" column="EM_EXPERT_ABILITY_GRADE"/>		</resultMap> 		<select id="getExpertAbilityHistoryByPrimKey" resultMap="expertAbilityHistoryMap" parameterClass="string">	  select EM_EXPERT_CODE,	             EM_EXPERT_ABILITY_DESC,               EM_EXPERT_RATING_START_DATE,               EM_EXPERT_RATING_END_DATE,               EM_EXPERT_SUPERIORITY,               EM_EXPERT_IMPROVEMENT,               ZC_INPUT_CODE,               (select USER_NAME from AS_USER U where USER_ID = D.ZC_INPUT_CODE) ZC_INPUT_NAME,                ZC_INPUT_DATE,                EM_EXPERT_ABILITY_GRADE	  	  	from ZC_EM_EXPERT_ABILITY_HISTORY d	 	where d.EM_EXPERT_CODE = #emExpertCode#	</select>  <insert id="addExpertAbilityHistory" parameterClass="EmExpertAbilityHistory" >			insert into ZC_EM_EXPERT_ABILITY_HISTORY( 	       EM_EXPERT_CODE,                  EM_EXPERT_ABILITY_DESC,                  EM_EXPERT_RATING_START_DATE,                  EM_EXPERT_RATING_END_DATE,                  EM_EXPERT_SUPERIORITY,                  EM_EXPERT_IMPROVEMENT,                  ZC_INPUT_CODE,                  ZC_INPUT_DATE,                  EM_EXPERT_ABILITY_GRADE      ) values (	         #emExpertCode#,                                #emExpertAbilityDesc#,                         #emExpertRatingStartDate#,                       #emExpertRatingEndDate#,                       #emExpertSuperiority#,                         #emExpertImprovement#,                         #zcInputCode#,                                 #zcInputDate#,                                 #emExpertAbilityGrade#        			) 	  </insert>  <delete id="deleteEmExpertAbilityHistory" parameterClass="string" >		delete from ZC_EM_EXPERT_ABILITY_HISTORY		where EM_EXPERT_CODE = #emExpertCode#	</delete>		<select id="selectByPrimaryKey" resultMap="expertAbilityMap" parameterClass="String">				 SELECT 		 		    EM_EXPERT_CODE,             EM_EXPERT_NAME,             EM_EXPERT_SEX,             EM_BIRTHDAY,                EM_ID_NO,                   EM_UNIT_NAME            FROM ZC_EM_B_EXPERT                             where                                                                                                                                        EM_EXPERT_CODE = #value#          </select></sqlMap>